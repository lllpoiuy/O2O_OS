{
    "agent_name" : "sac_bc",
    "lr" : 0.0003,
    "batch_size" : 256,
    "actor_hidden_dims": [512, 512, 512, 512],
    "value_hidden_dims": [512, 512, 512, 512],
    "critic_layer_norm": true,
    "actor_layer_norm": false,
    "discount": 0.99,
    "tau": 0.005,
    "target_update_interval": 1,
    "num_qs": 2,
    "horizon_length": null,
    "RSNorm": false,

    "actor_loss": {
        "type": "flow_bc",
        "alpha": 100.0,
        "OBAC": 10.0
    },

    "critic_loss": {
        "type": "sac",
        "q_agg": "mean",
        "cql": {
            "cql_n_actions": 10,
            "cql_temperature": 1,
            "cql_min_q_weight": 0.005,
            "cql_min_q_weight_online": 0.0,
            "cql_target_action_gap": null
        }
    },

    "sample_actions":{
        "type": "distill_ddpg",
        "actor_num_samples": 1,
        "soft_max": false,
        "imitation_boostrapped": false
    },
    
    "create_network": {
        "type": "flow",
        "flow_steps": 10,
        "use_fourier_features": false,
        "fourier_feature_dim": 64
    },

    "encoder" : null
}